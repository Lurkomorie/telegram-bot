# Telegram Bot
BOT_TOKEN=your_telegram_bot_token_here
WEBHOOK_SECRET=generate_random_secret_here
PUBLIC_BASE_URL=https://your-app.railway.app

# Database (Railway PostgreSQL - separate from main app)
DATABASE_URL=postgresql://postgres:your_password@your_host:port/railway

# Redis (using your existing Upstash Redis)
REDIS_URL=redis://default:your_redis_password@your_redis_host:6379

# AI Services
OPENROUTER_API_KEY=your_openrouter_api_key_here
RUNPOD_API_KEY_POD=your_runpod_api_key_here
RUNPOD_ENDPOINT=https://your_runpod_endpoint.proxy.runpod.net/run

# Security
IMAGE_CALLBACK_SECRET=your_callback_secret_here

# Admin Panel
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change_this_secure_password
ADMIN_SECRET_KEY=generate_random_secret_key_here

# Environment
ENV=production
LOG_LEVEL=INFO


